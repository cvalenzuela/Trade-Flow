  "repository": {
    "type": "git",
    "url": "git+https://github.com/cvalenzuela/Exporta-Web.git"
  },
  "keywords": [
    "visualization"
  ],
  "author": "Cristobal Valenzuela <cvalenzuela@nyu.edu> (https://github.com/cvalenzuela)",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/cvalenzuela/Exporta-Web/issues"
  },
  "homepage": "https://github.com/cvalenzuela/Exporta-Web#readme",
  "devDependencies": {
    "babel-cli": "6.24.1",
    "babel-loader": "7.0.0",
    "babel-preset-es2015": "6.24.1",
    "babel-preset-stage-2": "6.24.1",
    "babel-register": "6.24.1",
    "json-loader": "0.5.4",
    "rimraf": "2.6.1",
    "webpack": "2.6.1",
    "webpack-dev-server": "2.4.5"
  },
  "babel": {
    "presets": [
      "es2015",
      "stage-2"
    ]
  },
  "dependencies": {
    "d3-array": "1.2.0",
    "d3-collection": "1.0.4",
    "d3-scale": "4.0.2",
    "dat.gui": "0.6.1",
    "is-mobile": "0.2.2",
    "p5": "0.5.11",
    "startaudiocontext": "1.2.1",
    "tone": "0.10.0",
    "underscore": "1.8.3"
  }
